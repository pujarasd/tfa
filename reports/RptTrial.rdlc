        </Field>
        <Field Name="balance">
          <DataField>balance</DataField>
          <rd:TypeName>System.Decimal</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AccountsConnectionString</DataSourceName>
        <CommandText>SELECT        AccId, Ledger, GroupName, SUM(debit) AS debit, SUM(credit) AS credit, SUM(debit - credit) AS balance
FROM            (SELECT        a.AccId, c.GroupName, b.Ledger, (CASE type WHEN 'Dr' THEN amount ELSE 0 END) AS debit, (CASE type WHEN 'Cr' THEN amount ELSE 0 END) 
                                                    AS credit
                          FROM            tDataMain AS a INNER JOIN
                                                    mLedger AS b ON b.Id = a.AccId INNER JOIN
                                                    mLedgerGroup AS c ON c.Id = b.GroupId
                          WHERE        (a.Voucher LIKE '%bank%') OR
                                                    (a.Voucher LIKE '%cash%')) AS a_2
GROUP BY AccId, Ledger, GroupName
UNION
SELECT        AccId, Ledger, GroupName, SUM(debit) AS debit, SUM(credit) AS credit, SUM(debit - credit) AS balance
FROM            (SELECT        a.AccId, c.GroupName, b.Ledger, (CASE type WHEN 'Dr' THEN amount ELSE 0 END) AS debit, (CASE type WHEN 'Cr' THEN amount ELSE 0 END) 
                                                    AS credit
                          FROM            tdataDetails AS a INNER JOIN
                                                    mLedger AS b ON b.Id = a.AccId INNER JOIN
                                                    mLedgerGroup AS c ON c.Id = b.GroupId) AS a_1
GROUP BY AccId, Ledger, GroupName
ORDER BY AccId</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
