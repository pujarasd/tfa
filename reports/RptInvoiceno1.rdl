  <DataSets>
    <DataSet Name="DataSet1">
      <Query>
        <DataSourceName>DataSource1</DataSourceName>
        <CommandText>SELECT l.ledger
  ,ld.Address
,lc.city
,ld.Pin
  ,c.Challan
  ,c.date
  ,c.lrno
  ,d.ChallanId
  ,d.Grade
  ,d.Bags
  ,d.Weight
  ,d.Rate
  ,d.Amount
  ,d.Remark
  ,d.Description
,tc.city ToCity
,fc.city fromCity
,pc.city paycity
FROM   ttranschallanDetails d
inner join  ttranschallan c on   c.id = d.challanid
inner join mledger l on l.id  = c.custid 
inner join mledgerdetails ld on ld.ledgerid = l.id
inner join mcity lc on lc.id = ld.cityid
inner join mcity tc on tc.id = c.cityto
inner join mcity fc on fc.id = c.cityfrom
inner join mcity pc on pc.id = c.PayableAt 
</CommandText>
        <rd:DesignerState><QueryDefinition xmlns="http://schemas.microsoft.com/ReportingServices/QueryDefinition/Relational"><SelectedColumns><ColumnExpression ColumnOwner="ttranschallanDetails" ColumnName="ChallanId" /><ColumnExpression ColumnOwner="ttranschallanDetails" ColumnName="Grade" /><ColumnExpression ColumnOwner="ttranschallanDetails" ColumnName="Bags" /><ColumnExpression ColumnOwner="ttranschallanDetails" ColumnName="Weight" /><ColumnExpression ColumnOwner="ttranschallanDetails" ColumnName="Rate" /><ColumnExpression ColumnOwner="ttranschallanDetails" ColumnName="Amount" /><ColumnExpression ColumnOwner="ttranschallanDetails" ColumnName="Remark" /><ColumnExpression ColumnOwner="ttranschallanDetails" ColumnName="Description" /></SelectedColumns></QueryDefinition></rd:DesignerState>
